<div class="nested-fields well well-compact">
    <div class="form-group">
	    <%= f.label :disc_name %>
		<%= f.text_field :disc_name %>
		<%= link_to_remove_association "discを削除", f, class: 'btn btn-primary' %>

		 	<%= f.fields_for ([:admin, @disc]) do |song| %>
	            <p>song</p>
	            <%= render 'song_fields', f: song %>
	        <% end %>
	        <%= link_to_add_association 'songを追加', f,:songs, class: 'btn btn-primary' %>
    </div>
</div>